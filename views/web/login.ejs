<%- include("partials/header"); %>

<div class="container" style="padding-top: 100px;">
  <div class="container" style="width: 320px; padding: 45px 25px; box-shadow: 0 0 30px rgba(0, 0, 0, 0.15); border-radius: 8px;">
    <h1>Login</h1>
    <hr class="mb-5">
    <form action="/admin-webpdesa/dashboard-admin" method="POST">
      <div class="mb-3">
        <label for="exampleInputEmail1" class="form-label">Email address</label>
        <input type="email" class="form-control" id="exampleInputEmail1" name="email">
      </div>
      <div class="mb-5">
        <label for="exampleInputPassword1" class="form-label">Password</label>
        <input type="password" class="form-control" id="exampleInputPassword1" name="password">
      </div>
      <button style="width: 100%;" type="submit" class="btn btn-primary">Sign In</button>
    </form>
  </div>
</div>

<%- include("partials/footer"); %>